#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/dario/Documents/dev workspace/opensourceframework/node_modules/.pnpm/tsup@8.5.1_@swc+core@1.15.11_@swc+helpers@0.5.15__jiti@2.6.1_postcss@8.5.6_typescript@5.9.3_yaml@2.8.2/node_modules/tsup/dist/node_modules:/home/dario/Documents/dev workspace/opensourceframework/node_modules/.pnpm/tsup@8.5.1_@swc+core@1.15.11_@swc+helpers@0.5.15__jiti@2.6.1_postcss@8.5.6_typescript@5.9.3_yaml@2.8.2/node_modules/tsup/node_modules:/home/dario/Documents/dev workspace/opensourceframework/node_modules/.pnpm/tsup@8.5.1_@swc+core@1.15.11_@swc+helpers@0.5.15__jiti@2.6.1_postcss@8.5.6_typescript@5.9.3_yaml@2.8.2/node_modules:/home/dario/Documents/dev workspace/opensourceframework/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/dario/Documents/dev workspace/opensourceframework/node_modules/.pnpm/tsup@8.5.1_@swc+core@1.15.11_@swc+helpers@0.5.15__jiti@2.6.1_postcss@8.5.6_typescript@5.9.3_yaml@2.8.2/node_modules/tsup/dist/node_modules:/home/dario/Documents/dev workspace/opensourceframework/node_modules/.pnpm/tsup@8.5.1_@swc+core@1.15.11_@swc+helpers@0.5.15__jiti@2.6.1_postcss@8.5.6_typescript@5.9.3_yaml@2.8.2/node_modules/tsup/node_modules:/home/dario/Documents/dev workspace/opensourceframework/node_modules/.pnpm/tsup@8.5.1_@swc+core@1.15.11_@swc+helpers@0.5.15__jiti@2.6.1_postcss@8.5.6_typescript@5.9.3_yaml@2.8.2/node_modules:/home/dario/Documents/dev workspace/opensourceframework/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tsup/dist/cli-node.js" "$@"
else
  exec node  "$basedir/../tsup/dist/cli-node.js" "$@"
fi
