#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/dario/Documents/dev workspace/opensourceframework/node_modules/.pnpm/msw@0.28.2_encoding@0.1.13/node_modules/msw/cli/node_modules:/home/dario/Documents/dev workspace/opensourceframework/node_modules/.pnpm/msw@0.28.2_encoding@0.1.13/node_modules/msw/node_modules:/home/dario/Documents/dev workspace/opensourceframework/node_modules/.pnpm/msw@0.28.2_encoding@0.1.13/node_modules:/home/dario/Documents/dev workspace/opensourceframework/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/dario/Documents/dev workspace/opensourceframework/node_modules/.pnpm/msw@0.28.2_encoding@0.1.13/node_modules/msw/cli/node_modules:/home/dario/Documents/dev workspace/opensourceframework/node_modules/.pnpm/msw@0.28.2_encoding@0.1.13/node_modules/msw/node_modules:/home/dario/Documents/dev workspace/opensourceframework/node_modules/.pnpm/msw@0.28.2_encoding@0.1.13/node_modules:/home/dario/Documents/dev workspace/opensourceframework/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/msw@0.28.2_encoding@0.1.13/node_modules/msw/cli/index.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/msw@0.28.2_encoding@0.1.13/node_modules/msw/cli/index.js" "$@"
fi
